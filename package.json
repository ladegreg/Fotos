  "bugs": {
    "url": "https://github.com/ladegreg/MERN-Stack-App2/issues"
  },
  "homepage": "https://ladegreg.github.io/MERN-Stack-App2/",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "gh-pages": "^2.0.1",
    "graphiql": "^0.12.0",
    "graphql": "^14.1.1",
    "jsonwebtoken": "^8.4.0",
    "mongoose": "^5.4.9",
    "openode": "^1.9.1"
  },
  "devDependencies": {
    "nodemon": "^1.18.9"
  }
